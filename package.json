{
    "name": "obsidian-admonition",
    "version": "6.9.3",
    "description": "Admonition block-styled content for Obsidian.md",
    "main": "main.js",
    "scripts": {
        "rollup:dev": "rollup --config rollup.config-dev.js -w",
        "rollup:publish": "rollup --config rollup.publish.js -w",
        "rollup:build": "rollup --config rollup.config.js",
        "dev": "set NODE_OPTIONS=--openssl-legacy-provider && webpack --config webpack.dev.js -w",
        "publish": "webpack --config webpack.publish.js",
        "build": "npm run publish && webpack",
        "release": "standard-version"
    },
    "standard-version": {
        "t": "",
        "releaseCommitMessageFormat": "Obsidian Admonitions Release: v{{currentTag}}"
    },
    "keywords": [],
    "author": "Jeremy Valentine",
    "license": "MIT",
    "devDependencies": {
        "@codemirror/language": "^0.19.7",
        "@codemirror/rangeset": "^0.19.5",
        "@codemirror/stream-parser": "https://github.com/lishid/stream-parser",
        "@codemirror/text": "^0.19.5",
        "@codemirror/view": "^0.19.37",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-regular-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@popperjs/core": "^2.9.2",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-typescript": "^6.0.0",
        "@types/node": "^14.14.2",
        "@types/object.fromentries": "^2.0.0",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.2.0",
        "dotenv": "^10.0.0",
        "esbuild": "^0.14.11",
        "mini-css-extract-plugin": "^2.1.0",
        "object.fromentries": "^2.0.4",
        "obsidian": "^0.13.11",
        "raw-loader": "^4.0.2",
        "rollup": "^2.32.1",
        "rollup-plugin-css-only": "^3.1.0",
        "ts-loader": "^9.2.4",
        "tslib": "^2.0.3",
        "typescript": "^4.0.3",
        "webpack": "^5.47.1",
        "webpack-cli": "^4.7.2"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "standard-version": "^9.3.2"
    }
}